root verbosity = Break  # Print, Info, Warning, Error, Break, Syserror, Fatal
era = Run2
runBlock = 
# tree path = /nfs/data38/cms/mulholland/lpcTrees/Skims/Run2ProductionV12/ # Colorado, owned by mulholland
# tree path = /nfs/data38/cms/wtford/lpcTrees/Skims/Run2ProductionV12/  # Colorado, owned by wtford (Zjets only)
tree path = root://cmseos.fnal.gov//store/user/lpcsusyhad/SusyRA2Analysis2015/Skims/Run2ProductionV18/  # xrootd
# tree path = /eos/uscms/store/user/lpcsusyhad/SusyRA2Analysis2015/Skims/Run2ProductionV17/  # from cmslpc
verbosity = 2
# tree path = /mnt/hadoop/store/user/aperloff/SusyRA2Analysis2015/Skims/Run2ProductionV17/  # local store
root file index = ../datFiles/skimFilesV18.dat
# verbosity = 1  # ntuple
# tree path = root://cmseos.fnal.gov//store/user/lpcsusyhad/SusyRA2Analysis2015/Run2ProductionV17/  # ntuples, xrootd
# root file index = ../datFiles/ntupleFilesV17.dat  # ntuple
delta phi sample = nominal  # nominal, ldp, hdp, ldpnominal
integrated luminosity = 35.9
apply Z/gamma Pt cut = true
apply Z mass cut = true
min Nb cut = 1
use DeepCSV = true
apply b-tag SF = false
apply pileup weight = false
use custom pileup weight = true
apply HEM jet veto = true
apply Z Pt weight = true
apply double-ratio fit weight = true
apply scale factors to MC = false
restrict cleanVars for raw ntuple = false
